    "prop-types": "^15.6.2",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "prettier": "^1.19.1",
    "react-dom": "^16.12.0",
    "string-replace-loader": "^2.2.0",
    "style-loader": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
